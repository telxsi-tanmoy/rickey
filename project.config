[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/tanmoy11
[access "refs/for/refs/*"]
	read = group user/tanmoy11
	push = group user/tanmoy11
	label-Code-Review = -2..+2 group user/tanmoy11
	label-Verified = -1..+1 group user/tanmoy11
	rebase = group user/tanmoy11
	removeReviewer = group user/tanmoy11
	submit = group user/tanmoy11
[access "refs/heads/*"]
	read = group user/tanmoy11
	create = group user/tanmoy11
	push = +force group user/tanmoy11
	pushMerge = group user/tanmoy11
[access "refs/tags/*"]
	push = +force group user/tanmoy11
	pushTag = group user/tanmoy11
	pushSignedTag = group user/tanmoy11
